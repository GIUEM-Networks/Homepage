{
  "name": "@giuem-networks/homepage",
  "version": "0.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "parcel src/index.html",
    "clean": "del dist .cache",
    "prebuild": "yarn run clean",
